Ncpu = 0

[clip]
operation = CLIP
soltab = sol000/amplitude000
mode = below
clipLevel = 0.5
pol=[XX,YY]

[clip2]
operation = CLIP
soltab = sol000/amplitude000
mode = above
clipLevel = 2
pol=[XX,YY]

# use [time,freq] instead of just freq: it's much faster
[flagextendF]
operation = FLAGEXTEND
soltab = sol000/amplitude000
axesToExt = [time,freq]
size = [1,0]
percent = 50
maxCycles = 1
