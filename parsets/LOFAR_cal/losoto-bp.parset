Ncpu = 0

[duplicateS]
operation = DUPLICATE
soltab = sol000/amplitude000
soltabOut = amplitudeSmooth

[smooth]
operation = SMOOTH
soltab = sol000/amplitudeSmooth
pol = XX, YY
axesToSmooth = [time]
mode = median
log = True

#[flagstation]
#operation = FLAGSTATION
#soltab = sol000/amplitudeSmooth
#mode = bandpass
#ampRange = [0.3,2.0]
#skipInternational = True

[plotAs]
operation = PLOT
soltab = sol000/amplitudeSmooth
axesInPlot = [time,freq]
axisInTable = ant
plotFlag = True
prefix = plots/ampSmooth_
#minmax = [100,300]

[plotAs2]
operation = PLOT
soltab = sol000/amplitudeSmooth
axesInPlot = freq
time.minmaxstep = [0,1e30,10000000]
axisInTable = ant
axisInCol = pol
plotFlag = True
prefix = plots/ampSmooth_

[dublicateR]
operation = DUPLICATE
soltab = sol000/amplitude000
soltabOut = amplitudeRes

[residual]
operation = RESIDUALS
soltab = sol000/amplitudeRes
soltabsToSub = amplitudeSmooth
ratio = True

[plotAr1]
operation = PLOT
soltab = sol000/amplitudeRes
pol = XX,YY
freq.minmaxstep = [0,300e6,200]
axesInPlot = time
axisInTable = ant
axisInCol = pol
plotFlag = True
prefix = plots/ampSmoothRes_
NColFig = 4
figSize = [20,20]
minmax = [-1,1]

[plotAr2]
operation = PLOT
soltab = sol000/amplitudeRes
pol = XX,YY
time.minmaxstep = [0,1e20,500]
axesInPlot = freq
axisInTable = ant
axisInCol = pol
plotFlag = True
prefix = plots/ampSmoothRes_
minmax = [-1,1]

[plotAr3]
operation = PLOT
soltab = sol000/amplitudeRes
pol = XX,YY
axesInPlot = [time,freq]
axisInTable = ant
plotFlag = True
prefix = plots/ampSmoothRes_
minmax = [-0.5,0.5]

